{
  "configVersion": "v3",
  "app": {
    "appId": 1087646,
    "appName": "Dino Run",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.1.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Classic T-Rex runner game for Zepp OS"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.0",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "default": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/setting"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Dino Run"
    }
  },
  "defaultLanguage": "en-US"
}
